{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "modules": "auto"
        },
        "transform-runtime": {
          "useESModules": false
        }
      }
    ]
  ],
  "plugins": [
    "babel-plugin-styled-components",
    "lodash",
    [
      "react-intl",
      {
        "messagesDir": "./dist/messages/"
      }
    ]
  ],
  "env": {
    "development": {
      "compact": false
    },
    "production": {
      "plugins": [
        [
          "transform-remove-console",
          {
            "exclude": ["error"]
          },
        ],
        [
          "transform-react-remove-prop-types",
          {
            "removeImport": true
          }
        ],
        ["react-remove-properties", { "properties": ["data-cy"] }]
      ]
    }
  }
}
